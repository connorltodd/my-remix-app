{
  "name": "my-remix-app",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "storybook": "concurrently npm:watch:*",
    "watch:tailwind": "npx tailwind -i ./app/tailwind.css -o ./app/styles/tailwind.css --watch",
    "build:tailwind": "npx tailwind -i ./app/tailwind.css -o ./app/styles/tailwind.css",
    "watch:storybook": "storybook dev -p 6006",
    "build-storybook": "npm run build:tailwind && storybook build"
  },
  "dependencies": {
    "@remix-run/node": "^2.4.0",
    "@remix-run/react": "^2.4.0",
    "@remix-run/serve": "^2.4.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "isbot": "^3.6.13",
    "match-sorter": "^6.3.1",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sort-by": "^1.2.0",
    "tiny-invariant": "^1.3.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.3.1",
    "@storybook/addon-essentials": "^7.6.5",
    "@storybook/addon-interactions": "^7.6.5",
    "@storybook/addon-links": "^7.6.5",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/addon-styling-webpack": "^0.0.5",
    "@storybook/blocks": "^7.6.5",
    "@storybook/react": "^7.6.5",
    "@storybook/react-webpack5": "^7.6.5",
    "@storybook/test": "^7.6.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.6.15",
    "postcss-loader": "^7.3.3",
    "storybook": "^7.6.5",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
